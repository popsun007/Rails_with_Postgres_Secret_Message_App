<!DOCTYPE html>
<html>
<head>
  <title>Secrets</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if current_user %>
	<a href="/log_out">Log Out</a>
<% end %>
<%= yield %>

<% if flash[:errors] %>
	<% flash[:errors].each do |e| %>
	<p style="color:red;"><%= e %></p>
	<% end %>
<% end %>
</body>
</html>
